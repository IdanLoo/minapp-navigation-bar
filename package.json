{
  "name": "minapp-navigation-bar",
  "version": "0.1.1",
  "description": "小程序自定义导航栏刘海屏通用适配方案",
  "main": "miniprogram_dist/index.js",
  "scripts": {
    "dev": "gulp dev --develop",
    "watch": "gulp watch --develop --watch",
    "build": "gulp",
    "dist": "npm run build",
    "clean-dev": "gulp clean --develop",
    "clean": "gulp clean",
    "test": "jest ./test/* --silent --bail",
    "coverage": "jest ./test/* --coverage --bail"
  },
  "miniprogram": "miniprogram_dist",
  "jest": {
    "testEnvironment": "jsdom",
    "testURL": "https://jest.test",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IdanLoo/minapp-navigation-bar"
  },
  "author": "Idan Loo &lt;im@siwei.lu&gt;",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-env": "^1.7.0",
    "colors": "^1.3.1",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-if": "^2.0.2",
    "gulp-install": "^1.1.0",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "jest": "^24.7.1",
    "miniprogram-simulate": "^1.0.0",
    "through2": "^2.0.3",
    "vinyl": "^2.2.0",
    "webpack": "^4.29.5",
